.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.category {
  display: flex;
}
.category navigator {
  flex: 1;
}
.category navigator image {
  padding: 15rpx;
  width: 100%;
}
.floorList .floor-item {
  padding: 10rpx;
}
.floorList .floor-item .floor-title image {
  width: 100%;
}
.floorList .floor-item .floor-content {
  overflow: hidden;
}
.floorList .floor-item .floor-content navigator {
  float: left;
  width: 33.33%;
}
.floorList .floor-item .floor-content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floorList .floor-item .floor-content navigator:nth-child(2),
.floorList .floor-item .floor-content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floorList .floor-item .floor-content navigator image {
  width: 100%;
  height: 100%;
}
